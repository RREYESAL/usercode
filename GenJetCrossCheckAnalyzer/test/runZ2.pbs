echo $SHELL
cd $PBS_O_WORKDIR
pwd
source /cvmfs/cms.cern.ch/cmsset_default.sh
export SCRAM_ARCH=slc5_amd64_gcc462
eval `scramv1 runtime -sh`

cmsRun anaGenJet_cfg.py output="$Z2_OUTPUT" maxEvents="$Z2_MAXEVENTS" processType="$Z2_PROCESSTYPE" 

exit 0
